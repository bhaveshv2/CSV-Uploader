<div ><input type="text" class="search form-control border border-dark" placeholder="Search the keyword" ></div>


<div id="file-table">
    <% if(json && json.length>0){
        let keys = Object.keys(json[0]);%>
        <table id="data-container" class="table table-dark table-hover table-sm table-fixed">
            <h1><%=name%></h1>
            <thead class="thead-light">
                <tr>
                    <% for(let i=0;i< keys.length;i++){%>
                        <th scope="col">
                            <%=keys[i]%> <i class="fa fa-sort" onclick="sort('<%= i %>')"></i>
                        </th>
                    <%}%>
                </tr>
            </thead>
            <tbody>
                <%for(j of json){%>
                    <tr>
                        <% for(key in j){%>
                            <td><%=j[key]%></td>
                        <%}%>
                    </tr>
                <%}%>
            </tbody>
        </table>
    <%}%>
</div>